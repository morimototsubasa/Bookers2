<h2>User info</h2>
<%= form_with model: @user do |f| %>
  <label for="inputName">Name</label>
  <%= f.text_field :name %>
  <label for="inputImage">Image</label>
  <%= f.file_field :profile_image %>  
  <div class="body">Introduction</div>
  <%= f.text_area :introduction %>
  <%= f.submit "Update user" %>
<% end %>